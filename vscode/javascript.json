{
  "Set the let variable": {
    "prefix": "lett",
    "body": ["let $1 = $2;", "$3"],
    "description": "Set the let variable"
  },
  "Set the const variable": {
    "prefix": "cons",
    "body": ["const $1 = $2;", "$3"],
    "description": "Set the const variable"
  },
  "Print to console": {
    "prefix": "csl",
    "body": ["console.log(`$1`);", "$2"],
    "description": "Log output to console"
  },
  "Throw new Error": {
    "prefix": "thE",
    "body": ["throw new Error(`$1`);", "$2"],
    "description": "Throw new Error"
  },
  "Get DOMElement by querySelector": {
    "prefix": "docq",
    "body": ["const $1 = document.querySelector(`$2`);", "$3"],
    "description": "Get DOMElement by querySelector"
  },
  "Get DOMElements by querySelectorAll": {
    "prefix": "docqs",
    "body": ["const $1 = document.querySelectorAll(`$2`);", "$3"],
    "description": "Get DOMElements by querySelectorAll"
  },
  "Get DOMElement by getElementById": {
    "prefix": "doci",
    "body": ["const $1 = document.getElementById(`$2`);", "$3"],
    "description": "Get DOMElement by getElementById"
  }
}
