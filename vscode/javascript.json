{
  "Set the let variable": {
    "prefix": "lett",
    "body": ["let $1 = $2;", "$3"],
    "description": "Set the let variable"
  },
  "Set the const variable": {
    "prefix": "cons",
    "body": ["const $1 = $2;", "$3"],
    "description": "Set the const variable"
  },
  "Print to console": {
    "prefix": "csl",
    "body": ["console.log(`$1`);", "$2"],
    "description": "Log output to console"
  },
  "Throw new Error": {
    "prefix": "thE",
    "body": ["throw new Error(`$1`);", "$2"],
    "description": "Throw new Error"
  },
  "DOM querySelector": {
    "prefix": "docq",
    "body": ["document.querySelector(`$1`);", "$2"],
    "description": "DOM: document.querySelector"
  },
  "DOM getElementById": {
    "prefix": "doci",
    "body": ["document.getElementById(`$1`);", "$2"],
    "description": "DOM: document.getElementById"
  }
}
